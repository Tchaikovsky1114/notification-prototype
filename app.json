{
  "expo": {
    "name": "성원애드피아",
    "slug": "notification-prototype",
    "version": "1.0.1",
    
    "backgroundColor": "#000",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ebc8b7f7-8735-49b9-bd32-626d3629b62f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets."
      },
      "bundleIdentifier": "com.swadpia.notificationprototype"
    },
    "android": {
      "package": "com.swadpia.notificationprototype",
      "versionCode": 2,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "permissions": [
        "android.permission.NOTIFICATIONS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/swadpia.png",
          "color": "#000fff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "게시글에 사진을 등록하기 위해 접근을 허용하시겠어요?"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ebc8b7f7-8735-49b9-bd32-626d3629b62f"
      }
    },
    "owner": "sungwonadpia",
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
